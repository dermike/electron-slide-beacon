{
  "name": "slide-beacon-app",
  "version": "0.4.1",
  "description": "Electron app to broadcast URLs as an Eddystone Physical Web beacon",
  "main": "main.js",
  "scripts": {
    "package": "node ./node_modules/electron-packager/cli.js ./ SlideBeacon --platform=darwin --arch=x64 --electron-version=1.8.2 --ignore=node_modules/electron-rebuild --asar --icon=icon.icns --overwrite",
    "electron": "node ./node_modules/electron/cli.js .",
    "rebuild": "node ./node_modules/.bin/electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dermike/slide-beacon-app.git"
  },
  "keywords": [
    "electron"
  ],
  "author": "Mikael Jergefelt",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "11.0.1",
    "electron": "1.8.2",
    "electron-rebuild": "1.7.3"
  },
  "dependencies": {
    "eddystone-beacon": "1.0.7",
    "mdns": "^2.3.4",
    "ws": "^4.0.0"
  }
}
